{
 "name": "{%= name %}",
  "homepage": "{%= homepage %}",
  "description": "{%= description %}",
  "version": "{%= version %}",
  "author": "{%= author %}",
  "copyright": "{%= copyright %}",
  "private": true,
  "repository": {
    "type": "git",
    "url": "{%= repository %}"
  },
  "engines": {
    "node": ">=6.x.x"
  },
  {%= dependencies %},
  "dependencies": {
    "cacherrr": "0.*",
    "codein": "0.*",
    "cssevents": "0.*",
    "easings.js": "0.*",
    "editrrr": "0.*",
    "fastclick": "^1.0.6",
    "faul": "0.*",
    "flugrid": "0.*",
    "httprom": "0.*",
    "insicht": "0.*",
    "izitin": "0.*",
    "jquery": "^3.2.1",
    "objectFitPolyfill": "^2.0.5",
    "promise-polyfill": "^6.0.2",
    "routrrr": "0.*",
    "scrrroll": "0.*",
    "tiny-msg-client": "0.*",
    "ujs": "0.*"
  },
  "main": "{%= taskrunner %}file.js",
  "scripts": {
    "start": "{%= taskrunner %}",
    "postinstall": "[ -z \"$npm_config_production\" ] && ./node_modules/.bin/bower install",
    "dev": "{%= taskrunner %} dev",
    "dist": "{%= taskrunner %} dist"
  },
  "license": "MIT"
}
